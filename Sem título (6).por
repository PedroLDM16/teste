programa {
  funcao inicio() {
    
    real altura, largura, comprimento, resultado

    escreva("Qual a altura da caixa d'água: ")
    leia(altura)

    escreva("Qual a largura da caixa d'água: ")
    leia(largura)

    escreva("Qual a comprimento da caixa d'água: ")
    leia(comprimento)


    resultado = largura * altura * comprimento

    escreva("\nO volume da caixa d'água é: ", resultado) 



  }
}
